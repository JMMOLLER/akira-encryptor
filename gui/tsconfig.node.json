{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/preload/**/*",
    "src/configs/**/*",
    "src/workers/**/*",
    "src/utils/**/*",
    "src/main/**/*",
    // "../types.d.ts",
    // "../core/**/*",
    "types.d.ts",
    "utils/**/*",
  ],
  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
    "moduleResolution": "Bundler",
    "noUnusedLocals": false,
    "baseUrl": "./src",
    "paths": {
      // "@core/*": ["../../core/*"],
      // "@workers/*": ["./workers/*"],
      "@gui/utils/*": ["./utils/*"],
      // "@libs/*": ["../../core/libs/*"],
      "@gui/configs/*": ["./configs/*"],
      // "@utils/*": ["../../core/utils/*"],
      // "@configs/*": ["../../core/configs/*"],
      // "@adapters/*": ["../../core/adapters/*"],
      // "@crypto/*": ["../../core/crypto/*"],
    },
  }
}
