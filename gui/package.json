{
  "name": "akira-encryptor-gui",
  "version": "0.4.1-beta",
  "description": "A GUI for Akira Encryptor",
  "main": "./out/main/index.mjs",
  "author": "JMMOLLER",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build:core-worker": "cd .. && npm run build:worker",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "7zip-bin": "^5.2.0",
    "@akira-encryptor/core": "file:..\\core\\akira-encryptor-core-0.4.2-beta.tgz",
    "@ant-design/icons": "^6.0.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@iconify-json/fluent-color": "^1.2.12",
    "@iconify/react": "^6.0.0",
    "@tailwindcss/vite": "^4.1.4",
    "antd": "^5.24.7",
    "bcryptjs": "^3.0.2",
    "electron-conf": "^1.3.0",
    "electron-updater": "^6.3.9",
    "moment": "^2.30.1",
    "node-7z": "^3.0.0",
    "tailwindcss": "^4.1.4",
    "tiny-uid": "^1.1.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^22.14.1",
    "@types/node-7z": "^2.1.10",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "deep-object-diff": "^1.1.9",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-devtools-installer": "^4.0.0",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "picocolors": "^1.1.1",
    "piscina": "^5.0.0",
    "prettier": "^3.5.3",
    "rc-menu": "^9.16.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-static-copy": "^3.0.0"
  }
}
